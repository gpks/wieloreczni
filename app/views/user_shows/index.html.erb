<table class="table table-hover">
   <thead>
    <tr>
    <th>User</th>
    <th>Number of games</th>
    

    </tr>
  </thead>

   <% @users.each do |user| %>
      <tr>
         <td><%= link_to "#{user.username}", user_show_path(user)  %></td>
          <td><%= user.games.count %></td>
        
      </tr>
    <% end %>
</table>